{
  "name": "tinyuploader",
  "version": "1.0.0",
  "workspaces": [
    "packages/*",
    "sdk"
  ],
  "description": "",
  "private": "true",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "pnpm run --parallel --filter @tinyuploader/* build",
    "dev:sdk": "pnpm -F @tinyuploader/sdk run dev",
    "build:sdk": "pnpm -F @tinyuploader/sdk run build",
    "dev:package-vue": "pnpm -F @tinyuploader/vue run dev",
    "build:package-vue": "pnpm -F @tinyuploader/vue run build",
    "dev:package-vuenext": "pnpm -F @tinyuploader/vuenext run dev",
    "build:package-vuenext": "pnpm -F @tinyuploader/vuenext run build",
    "dev:demo-vue": "pnpm run dev:sdk & pnpm run dev:package-vue & pnpm -F @demos/vue run dev",
    "dev:demo-vuenext": "pnpm run dev:sdk & pnpm run dev:package-vuenext & pnpm -F @demos/vuenext run dev",
    "change": "pnpm changeset",
    "change-version": "pnpm changeset version",
    "publish": "pnpm install && pnpm publish -r"
  },
  "keywords": [
    "uploader",
    "upload",
    "上传",
    "分片上传",
    "大文件",
    "并发",
    "重试",
    "断点续传",
    "秒传"
  ],
  "author": "",
  "license": "ISC",
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moyuderen/uploader.git"
  },
  "bugs": {
    "url": "https://github.com/moyuderen/uploader/issues"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.7",
    "@vitejs/plugin-vue": "^5.0.5",
    "npm-run-all": "^4.1.5",
    "vite": "^5.3.2",
    "vue": "^3.4.31"
  }
}
